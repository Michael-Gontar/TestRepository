  "SysTableOrgConds": [
    {
      "text": "SELECT \n    po.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n   PurOrders po\nJOIN \n    userapplications ua ON  ua.username in (\n        SELECT ur.UserName\n        FROM UserRoles ur\n        JOIN RolePrivileges rp ON rp.Role = ur.Role\n        WHERE rp.TableName = 'PurOrders')",
      "primarykey": "PurOrders",
      "fatherkey": "PurOrders",
      "ngv_modifydate": "2025-07-20T11:11:22.964017",
      "ngv_hashcode": "",
      "ord": 0
    }
  ],
