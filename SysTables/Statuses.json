          "ngv_modifydate": "2025-07-13T14:41:43.44336",
          "query": "select case when(\n    exists (\n        select 'x' from Statuses s\n        where s.Status = {ParentRecord.Status}\n        and s.TableName = {ParentRecord.TableName}\n        and s.FreePass = 1\n    )\n    or exists (\n        select 'x' from NextStatus ns\n        where ns.FatherKey = (select PrimaryKey\n            from Statuses s\n            where s.Status = {ParentRecord.Status}\n            and s.TableName = {ParentRecord.TableName})\n        and (ns.Role = ''  or ns.Role = {UserParams.Role})\n        and ns.Status = Statuses.Status\n    )\n)\nthen 1 else 0\nend\nwhere Statuses.TableName = {ParentRecord.TableName}\n    and Statuses.Status <> {ParentRecord.Status}",
          "tablename": "Statuses",
          "columnname": "Display"
        }
      ]
