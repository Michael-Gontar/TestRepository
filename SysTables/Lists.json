    {
      "text": "SELECT l.primarykey AS entityid,\n       cpv.userid AS userid,\n       cpv.applicationcode\nFROM lists l \nINNER JOIN commands c ON (l.primarykey = c.targetentityname) \nINNER JOIN commands_permissions_view cpv ON (cpv.entityid = c.primarykey)\nUNION\nSELECT l.primarykey AS entityid,\n       ua.username AS userid,\n       ua.applicationcode\nFROM lists l\nINNER JOIN userapplications ua ON l.primarykey IN ('UserGuides', 'ChangeList', 'ChangeDetails', 'Categories')",
