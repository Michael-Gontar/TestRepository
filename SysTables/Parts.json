  "SysTableOrgConds": [
    {
      "text": "SELECT \n    p.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n    Parts p\nJOIN \n    userapplications ua ON ua.username IN (\n        SELECT ur.UserName\n        FROM UserRoles ur\n        JOIN RolePrivileges rp ON rp.Role = ur.Role\n        WHERE rp.TableName = 'Parts'\n   )",
      "primarykey": "Parts",
      "fatherkey": "Parts",
      "ngv_modifydate": "2025-07-20T11:11:22.103227",
      "ngv_hashcode": "",
      "ord": 0
    }
  ],
