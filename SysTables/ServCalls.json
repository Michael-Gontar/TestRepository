  "SysTableOrgConds": [
    {
      "text": "SELECT \n    s.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n    ServCalls s\nJOIN \n    userapplications ua \n    ON (\n        ((SELECT value FROM UserParams WHERE UserName = ua.username AND UserAttributeName = 'IsManager' ) = 'true')\n      or\n((SELECT value FROM UserParams WHERE UserName = ua.username AND UserAttributeName = 'IsOffice' ) = 'true')\n        OR s.AssignedTo = ua.username\n    )\nWHERE \n    ua.username IN (\n        SELECT ur.UserName\n        FROM UserRoles ur\n        JOIN RolePrivileges rp ON rp.Role = ur.Role\n        WHERE rp.TableName = 'ServCalls'\n    )\nAND NOT EXISTS (\n    SELECT 1\n    FROM Statuses st\n    WHERE \n        st.Status = s.Status\n        AND st.TableName = 'ServCalls'\n        AND st.Canceled = true\n)\n\nUNION\n\n-- Retrieve from SearchData table by username and table\nSELECT \n    s.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n    SearchData sd\nJOIN \n    ServCalls s ON s.PrimaryKey = sd.Key1\nJOIN \n    userapplications ua ON ua.username = sd.UserName\nWHERE \n    sd.TableName = 'ServCalls'",
      "primarykey": "ServCalls",
      "fatherkey": "ServCalls",
      "ngv_modifydate": "2025-07-20T11:03:46.520237",
      "ngv_hashcode": "",
      "ord": 0
    }
  ],
