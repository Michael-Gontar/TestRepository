  "SysTableOrgConds": [
    {
      "text": "SELECT \n    gr.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n   GoodsReceived gr\nJOIN \n    userapplications ua ON gr.AssignedTo = ua.username\nWHERE \n    ua.username IN (\n        SELECT ur.UserName\n        FROM UserRoles ur\n        JOIN RolePrivileges rp ON rp.Role = ur.Role\n        WHERE rp.TableName = 'GoodsReceived'\n    )\nAND NOT EXISTS (\n    SELECT 1\n    FROM Statuses st\n    WHERE \n        st.Status = gr.Status\n        AND st.TableName = 'GoodsReceived'\n        AND st.Canceled = true\n)\nAND NOT EXISTS (\n    SELECT 1\n    FROM Statuses st\n    WHERE \n        st.Status = gr.Status\n        AND st.TableName = 'GoodsReceived'\n        AND st.Final = true\n)\n\n\nUNION\n\n-- Retrieve from SearchData table by username and table\nSELECT \n    gr.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n    SearchData sd\nJOIN \n    GoodsReceived gr ON gr.PrimaryKey = sd.Key1\nJOIN \n    userapplications ua ON ua.username = sd.UserName\nWHERE \n    sd.TableName = 'GoodsReceived'",
      "primarykey": "GoodsReceived",
      "fatherkey": "GoodsReceived",
      "ngv_modifydate": "2025-07-23T08:04:40.624189",
      "ngv_hashcode": "",
      "ord": 0
    }
  ],
