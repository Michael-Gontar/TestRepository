      "transferchanges": false,
      "SysTableColumnQuery": [
        {
          "primarykey": "Opports:IsEditable",
          "fatherkey": "Opports:IsEditable",
          "ngv_hashcode": "",
          "ngv_modifydate": "2025-07-20T09:13:00.155133",
          "query": "SELECT \n    CASE \n        WHEN EXISTS (\n            SELECT 1\n            FROM UserRoles ur\n            JOIN RolePrivileges rp ON rp.Role = ur.Role\n            WHERE ur.UserName = {UserParams.UserName}\n              AND rp.TableName = 'Opports'\n              AND rp.Editing = 1\n        )\n        THEN true\n        ELSE false\n    END",
          "tablename": "Opports",
          "columnname": "IsEditable"
        }
