  "SysTableOrgConds": [
    {
      "text": "SELECT \n    c.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n    Customers c\nJOIN \n    userapplications ua ON ua.username IN (\n        SELECT ur.UserName\n        FROM UserRoles ur\n        JOIN RolePrivileges rp ON rp.Role = ur.Role\n        WHERE rp.TableName = 'Customers'\n )",
      "primarykey": "Customers",
      "fatherkey": "Customers",
      "ngv_modifydate": "2025-07-20T11:11:22.557444",
      "ngv_hashcode": "",
      "ord": 0
