  "SysTableOrgConds": [
    {
      "text": "SELECT \n    c.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n    CustSpecPrices c\nJOIN \n    userapplications ua ON ua.username IN (\n        SELECT ur.UserName\n        FROM UserRoles ur\n        JOIN RolePrivileges rp ON rp.Role = ur.Role\n        WHERE rp.TableName = 'CustSpecPrices'\n    )\nWHERE \n    c.ExpiryDate >= CURRENT_DATE\n    and c.FromDate <= CURRENT_DATE",
      "primarykey": "CustSpecPrices",
      "fatherkey": "CustSpecPrices",
      "ngv_modifydate": "2025-07-20T11:11:22.521856",
      "ngv_hashcode": "",
      "ord": 0
    }
  ],
