      "transferchanges": false,
      "SysTableColumnQuery": [
        {
          "primarykey": "Orders:IsEditable",
          "fatherkey": "Orders:IsEditable",
          "ngv_hashcode": "",
          "ngv_modifydate": "2025-07-21T13:59:22.90587",
          "query": "SELECT\n    CASE\n        WHEN (EXISTS (\n            SELECT 1\n            FROM UserRoles ur\n            JOIN RolePrivileges rp ON rp.Role = ur.Role\n            WHERE ur.UserName = {UserParams.UserName}\n            AND rp.TableName = 'Orders'\n            AND rp.Editing = 1\n        )\n        AND EXISTS (\n            SELECT 1 \n            FROM Statuses ss\n            WHERE ss.Status = Orders.Status\n            AND ss.TableName = 'Orders'\n            AND ss.AllowEdit = 1\n        )\n        )\n        THEN true\n        ELSE false\n    END",
          "tablename": "Orders",
          "columnname": "IsEditable"
        }
      ]
    },
