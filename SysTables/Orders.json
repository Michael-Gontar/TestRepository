  "SysTableOrgConds": [
    {
      "text": "SELECT \n    ors.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n    Orders ors\nJOIN \n    userapplications ua \n    ON (\n        ((SELECT value FROM UserParams WHERE UserName = ua.username AND UserAttributeName = 'IsManager' ) = 'true')\n      or\n((SELECT value FROM UserParams WHERE UserName = ua.username AND UserAttributeName = 'IsOffice' ) = 'true')\n        OR ors.AssignedTo = ua.username\n    )\nWHERE \n    ua.username IN (\n        SELECT ur.UserName\n        FROM UserRoles ur\n        JOIN RolePrivileges rp ON rp.Role = ur.Role\n        WHERE rp.TableName = 'Orders'\n    )\nAND NOT EXISTS (\n    SELECT 1\n    FROM Statuses st\n    WHERE \n        st.Status = ors.Status\n        AND st.TableName = 'Orders'\n        AND st.Canceled = true\n)\n\nUNION\n\n-- Retrieve from SearchData table by username and table\nSELECT \n    ors.primarykey AS entityid,\n    ua.username AS userid,\n    ua.applicationcode\nFROM \n    SearchData sd\nJOIN \n    Orders ors ON ors.PrimaryKey = sd.Key1\nJOIN \n    userapplications ua ON ua.username = sd.UserName\nWHERE \n    sd.TableName = 'Orders'",
      "primarykey": "Orders",
      "fatherkey": "Orders",
      "ngv_modifydate": "2025-07-21T14:06:58.714939",
      "ngv_hashcode": "",
      "ord": 0
    }
  ],
